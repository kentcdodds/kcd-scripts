{
  "name": "cod-scripts",
  "version": "0.0.0-semantically-released",
  "description": "CLI for common scripts for my projects",
  "engines": {
    "node": ">=10.13",
    "npm": ">=6",
    "yarn": ">=1"
  },
  "bin": {
    "cod-scripts": "dist/index.js"
  },
  "scripts": {
    "test": "node src test",
    "test:update": "node src test --updateSnapshot",
    "build": "node src build",
    "lint": "node src lint",
    "commitlint": "node src commitlint",
    "format": "node src format",
    "validate": "node src validate"
  },
  "files": [
    "dist",
    "babel.js",
    "commitlint.js",
    "eslint.js",
    "config.js",
    "prettier.js",
    "jest.js"
  ],
  "keywords": [],
  "author": "Chris O'Donnell <dev@codfish.io> (http://codfish.io/)",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "arrify": "^2.0.1",
    "babel-jest": "^26.6.1",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-minify-dead-code-elimination": "^0.5.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "browserslist": "^4.14.6",
    "concurrently": "^5.3.0",
    "cosmiconfig": "^7.0.0",
    "cross-env": "^7.0.0",
    "cross-spawn": "^7.0.3",
    "doctoc": "^1.4.0",
    "eslint": "^7.12.1",
    "eslint-config-codfish": "^7.0.0",
    "glob": "^7.1.5",
    "husky": "^4.3.0",
    "is-ci": "^2.0.0",
    "jest": "^26.6.1",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.has": "^4.5.2",
    "lodash.omit": "^4.5.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.1.2",
    "react-app-polyfill": "^2.0.0",
    "read-pkg-up": "^7.0.1",
    "resolve": "^1.18.1",
    "rimraf": "^3.0.0",
    "rollup": "^2.33.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "semver": "^7.3.2",
    "which": "^2.0.2",
    "yargs-parser": "^20.2.3"
  },
  "devDependencies": {
    "jest-in-case": "^1.0.2",
    "slash": "^3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "./src/config/eslintrc.js"
    ],
    "rules": {
      "no-console": "off",
      "no-process-exit": "off",
      "global-require": "off",
      "no-useless-catch": "off",
      "import/no-dynamic-require": "off",
      "import/no-unassigned-import": "off",
      "no-nested-ternary": "off"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "node src pre-commit",
      "commit-msg": "node src commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "./src/config/commitlint.config"
    ],
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "eslint",
          "babel",
          "rollup",
          "commitlint",
          "jest",
          "lintstaged",
          "lint-staged",
          "husky",
          "prettier"
        ]
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codfish/cod-scripts.git"
  },
  "bugs": {
    "url": "https://github.com/codfish/cod-scripts/issues"
  },
  "homepage": "https://github.com/codfish/cod-scripts#readme"
}
